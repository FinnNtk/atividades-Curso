nome=input('Digite o nome do funcionario: ')
clas=float(input('Digite sua classificação: '))

if clas >=8 or clas ==10:
    nota="A"
    print('Nome do funcionario: ', nome, 'Nota: ',nota)
elif clas >=5 or clas ==7:
    nota="B"
    print('Nome do funcionario: ', nome, 'Nota: ',nota)
elif clas <5:
    nota="c"
    print('Nome do funcionario: ', nome, 'Nota: ',nota)
